{
    "name": "lawpro/system",
    "type": "project",
    "require": {
        "php": "^7.4",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "hesto/multi-auth": "^2.0",
        "wladmonax/laravel-db-backup": "^1.1",
        "fideloper/proxy": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "allow-plugins": {
            "*": true
        }
    },
    "audit": {
        "block-insecure": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
